<h1>Start a new track</h1>

<%= form_for(@song) do |f| %>

  <div class="field">
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </div>

  <div class="field">
    <%= f.label :tag_list, "Tags, each separated by a comma (,)" %><br>
    <%= f.text_field :tag_list %>
  </div><br>
  

  <div class="actions">
    <%= f.submit %>
  </div>
  <br>
<% end %>





